{
  "name": "willgriffiths.github.io",
  "description": "A playground for frontend experiments.",
  "version": "1.0.0",
  "author": "Will Griffiths",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "yarn test --changedSince=develop",
    "prepush": "yarn test",
    "deploy": "yarn precommit && gatsby build --prefix-paths && gh-pages -d public -b master"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "gatsby-plugin-eslint": "^1.0.3",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-runner-eslint": "^0.6.0",
    "prettier": "^1.12.0"
  },
  "jest": {
    "runner": "jest-runner-eslint",
    "displayName": "lint",
    "testMatch": [
      "<rootDir>/src/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/willgriffiths/willgriffiths.github.io"
  }
}
